{
  "compilerOptions": {
   "allowJs": true,
   "baseUrl": "./",
   "noEmit": true, 
   "target": "es2016",                              
   "module": "commonjs",                                
   "esModuleInterop": true,                           
   "forceConsistentCasingInFileNames": true,            
   "strict": true,                                     
   "skipLibCheck": true,
   "types": [
    "cypress",
    "@shelex/cypress-allure-plugin",
    "@types/node"
  ],
  "paths": {
    "@src/*": ["src/*"]
  },

  
  "resolveJsonModule": true,
                        
  }

}
